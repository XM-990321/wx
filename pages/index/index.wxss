.tt_swiper swiper {
  height: 340rpx;
}
.tt_swiper swiper image {
  width: 100%;
}
.tt_cate {
  display: flex;
}
.tt_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.tt_cate navigator image {
  width: 100%;
}
.tt_floor .floor_group .floor_title image {
  width: 100%;
  margin: 20rpx 0 5rpx 0;
}
.tt_floor .floor_group .floor_list navigator {
  float: left;
  width: 33.33%;
}
.tt_floor .floor_group .floor_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
}
.tt_floor .floor_group .floor_list navigator:nth-child(2),
.tt_floor .floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.tt_floor .floor_group .floor_list navigator:nth-child(2),
.tt_floor .floor_group .floor_list navigator:nth-child(4) {
  border-left: 10rpx solid #fff;
}
.tt_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
